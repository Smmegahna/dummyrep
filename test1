lower = int(input("Enter the lower value:"))
upper = int(input("Enter the upper value:"))
sum=0
for number in range(lower, upper+1):
    if number>=1:
        for i in range(2,number):
            if number%i==0:
                i=number
                break
        else:

                sum=sum+number

                print(number)
print(sum)
